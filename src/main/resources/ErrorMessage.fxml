<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <root>
      <AnchorPane prefHeight="350.0" prefWidth="350.0">
         <children>
            <BorderPane layoutX="15.0" layoutY="15.0" prefHeight="320.0" prefWidth="320.0">
               <top>
                  <HBox alignment="CENTER" prefHeight="39.0" prefWidth="290.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label fx:id="title" alignment="CENTER" prefHeight="40.0" prefWidth="181.0" text="ERROR" textFill="RED">
                           <font>
                              <Font name="YuGothic Bold" size="40.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </top>
               <center>
                  <Label fx:id="errorMessage" text="Error Message Here" textFill="RED" BorderPane.alignment="CENTER" />
               </center>
               <bottom>
                  <Button fx:id="closeError" mnemonicParsing="false" prefHeight="61.0" prefWidth="138.0" text="Ok im sorry" BorderPane.alignment="CENTER" />
               </bottom>
            </BorderPane>
         </children>
      </AnchorPane>
   </root>
</Scene>
